{
    "name": "yoanbernabeu/daplos-bundle",
    "description": "Bundle Symfony DaplosBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["symfony", "bundle", "daplos"],
    "authors": [
        {
            "name": "Yoan Bernabeu",
            "email": "contact@yoandev.co"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/http-kernel": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/config": "^6.4|^7.0",
        "symfony/http-client": "^6.4|^7.0",
        "symfony/cache": "^6.4|^7.0",
        "symfony/console": "^6.4|^7.0",
        "doctrine/orm": "^2.14|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "symfony/phpunit-bridge": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "YoanBernabeu\\DaplosBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YoanBernabeu\\DaplosBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

